const mongoose = require('mongoose');
const empSchema = mongoose.Schema({
    name:{
        type:String,
        required:true
    },
    emailid:{
        type:String,
        required:true
    },
    phone:{
        type:Number,
        required:true
    },
    designation:{
        type:String,
        required:true
    },
    companyname:{
        type:String,
        required:true
    },
    location:{
        type:String,
        required:true
    },
    username:{
        type:Number,
        required:true
    },
    password:{
        type:String,
        required:true
    }
});
const jobModel = mongoose.model('Jobdetails',jobSchema);
module.exports = jobModel;